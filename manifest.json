{
	"manifest_version": 2,
	"name": "Open Link in Immediately Adjacent Tab",
	"description": "Moves a newly opened child tab right next to its parent tab.",
	"version": "0.5",
	"applications":{
		"gecko": {
			"id": "open-tab-adjacent@jeffersonscher.com",
			"strict_min_version": "57.0"
		}
	},
	"developer": {
		"name": "Jefferson \"jscher2000\" Scher",
		"url": "https://github.com/jscher2000/open-tab-adjacent-extension"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"storage"
	],
	"icons": {
		"128": "icons/adjacent-128.png"
	}
}
